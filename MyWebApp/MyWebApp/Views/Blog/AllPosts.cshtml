@{
    @using MyWebApp.Models
    @using System.Collections.Generic
    @model List<Post>
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table>
    @foreach (Post p in Model)
    {
        <tr>
            <div>
                <h1>@p.Title  @p.Created</h1>
                <h2>@p.Content</h2>
            </div>
        </tr>
    }
</table>